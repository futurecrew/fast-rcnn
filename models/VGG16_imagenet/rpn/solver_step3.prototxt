train_net: "models/VGG16_imagenet/rpn/train_step3.prototxt"
#base_lr: 0.001
base_lr: 0.0005
lr_policy: "step"
gamma: 0.1
stepsize: 60000
display: 20
average_loss: 100
# iter_size: 1
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "VGG16_imagenet_rpn_step3"
#debug_info: true
